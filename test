#include <stdio.h>

int main(){
    int num, sum = 0;

    while(scanf("%d",&num) != EOF){
        if(num % 2 == 0 && num >= 10 && num < 100){
            sum += num;
        }
    }

    printf("Sum = %d\n", sum);
    return 0;
}

//============================================================

#include <stdio.h>
#include <string.h>

int main(int argc, char *argv[]){
    int num;

    while(scanf("%d", &num) != EOF){
        if(argc == 2){
            if(strcmp(argv[1], "-evens") == 0){
                if(num % 2 == 0){
                    printf("%d\n", num);
                }
            }else if(strcmp(argv[1], "-odds") == 0){
                if(num % 2 != 0){
                    printf("%d\n", num);
                }
            }else if(strcmp(argv[1], "-positives") == 0){
                if(num > 0){
                    printf("%d\n", num);
                }
            }else if(strcmp(argv[1], "-negatives") == 0){
                if(num < 0){
                    printf("%d\n", num);
                }
            }
        }else{
            printf("%d\n", num);
        }
    }
    return 0;
}

//=============================================================

#include <stdio.h>
#include <string.h>
#include <stdlib.h>

int main(int argc, char *argv[]){
    int num;

    while(scanf("%d", &num) != EOF){
        if(argc >= 2){
            if(strcmp(argv[1], "-evens") == 0  strcmp(argv[1], "-e") == 0){
                if(num % 2 == 0){
                    printf("%d\n", num);
                }
            }else if(strcmp(argv[1], "-odds") == 0  strcmp(argv[1], "-o") == 0){
                if(num % 2 != 0){
                    printf("%d\n", num);
                }
            }else if(strcmp(argv[1], "-positives") == 0  strcmp(argv[1], "-p") == 0){
                if(num > 0){
                    printf("%d\n", num);
                }
            }else if(strcmp(argv[1], "-negatives") == 0   strcmp(argv[1], "-n") == 0){
                if(num < 0){
                    printf("%d\n", num);
                }
            }else if(strcmp(argv[1], "-range") == 0  || strcmp(argv[1], "-r") == 0){
                if(num >= atoi(argv[2]) && num <= atoi(argv[3])){
                    printf("%d\n", num);
                }
            }
        }else{
            printf("%d\n", num);
        }
    }
    return 0;
}

//==========================================================================

#include <stdio.h>

#define PI 3.14
#define CIRCLEAREA(r)* (r) * (r) * PI

int main(){
    printf("%.2f\n", CIRCLEAREA(10));
    printf("%.2f\n", CIRCLEAREA(20));
    printf("%.2f\n", CIRCLEAREA(30));

    return 0;
}
